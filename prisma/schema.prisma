generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Uygulamalar {
  id         String @id @default(cuid())
  name       String
  açıklama String
  filePath   String
  fileName   String
  fileSize   Int
  fileType   String
}
